{
    "manifest_version": 2,
    "name": "enigma-dns",
    "version": "1.0",
    "description": "Uses enigma network for resolving .enigma addresses.",
    "icons": {
        "48": "icons/TODO.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.mozilla.org/*"],
            "js": ["enigma_dns.js"]
        }
    ]
}

